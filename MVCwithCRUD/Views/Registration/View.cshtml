@model IEnumerable<DataAccessLayer.Registration>

@{
    ViewData["Title"] = "View";
}

<h1>View</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RegistrationId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Password)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ConformPassword)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RegistrationId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Password)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ConformPassword)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new {id=item.RegistrationId}) |
                @Html.ActionLink("Details", "Details", new { id = item.RegistrationId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.RegistrationId })
            </td>
        </tr>
}
    </tbody>
</table>
